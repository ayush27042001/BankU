                        }
                    },
                },
                colors: ['#e4bd51'],
                dataLabels: {
                    enabled: false
                },

                series: [
                    {
                        name: 'Central',
                        data: generateDayWiseTimeSeries(new Date('14 Aug 2024 GMT').getTime(), 10, {
                            min: 20,
                            max: 48
                        })
                    }
                ],

                fill: {
                    type: 'gradient',
                    gradient: {
                        opacityFrom: 0.6,
                        opacityTo: 0.8,
                    }
                },

                legend: {
                    position: 'top',
                    horizontalAlign: 'right',
                    show: true,
                },
                xaxis: {
                    type: 'datetime',
                },
                grid: {
                    yaxis: {
                        lines: {
                            show: false,
                        }
                    },
                    padding: {
                        top: 0,
                        right: 0,
                        bottom: 0,
                        left: 0
                    },
                },
                stroke: {
                    show: true,
                    curve: 'smooth',
                    width: 2,
                },
            }

            var chart = new ApexCharts(document.querySelector("#movement-apex"), options);
            chart.render();
            function generateDayWiseTimeSeries(baseval, count, yrange) {
                var i = 0;
                var series = [];
                while (i < count) {
                    var x = baseval;
                    var y = Math.floor(Math.random() * (yrange.max - yrange.min + 1)) + yrange.min;

                    series.push([x, y]);
                    baseval += 86400000;
                    i++;
                }
                return series;
            }

        });

