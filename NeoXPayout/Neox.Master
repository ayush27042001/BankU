    <script>
        const sidebarToggle = document.getElementById("sidebarToggle");
        const unifiedToggle = document.getElementById("unifiedToggle");

        let isSidebarCollapsed = false;

        function setToFavicon() {
            unifiedToggle.innerHTML = `<img src="assets/images/apple-touch-icon.png" width="24" height="24" alt="Toggle Icon">`;
        }

        function setToSVG() {
            unifiedToggle.innerHTML = `
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
                 fill="none" stroke="currentColor" stroke-width="1" stroke-linecap="round"
                 stroke-linejoin="round">
                <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                <path d="M15 12h-12"></path>
                <path d="M7 8l-4 4l4 4"></path>
                <path d="M12 21a9 9 0 0 0 0 -18"></path>
            </svg>`;
        }

        // Toggle icon and sidebar state on click
        unifiedToggle.addEventListener("click", function (e) {
            e.preventDefault();
            isSidebarCollapsed = !isSidebarCollapsed;

            if (isSidebarCollapsed) {
                setToFavicon();
                document.querySelector(".sidebar")?.classList.add("collapsed");
            } else {
                setToSVG();
                document.querySelector(".sidebar")?.classList.remove("collapsed");
            }
        });

        // Optional: Handle mobile button
        sidebarToggle.addEventListener("click", function () {
            sidebarToggle.classList.add("d-none");
            isSidebarCollapsed = true;
            setToFavicon();
            document.querySelector(".sidebar")?.classList.add("collapsed");
        });
    </script>

    <script>
        let inactivityTimer;
        const inactiveLimit = 10 * 60 * 1000; // 10 minutes
        let isLocked = false;


        function triggerLock() {
            fetch('StoreLastPage.aspx?url=' + encodeURIComponent(window.location.pathname), {
                method: 'GET'
            }).then(() => {
                window.location.href = 'MPIN.aspx';
            }).catch(err => {
                console.error("Error locking session:", err);
                window.location.href = 'MPIN.aspx';
            });
        }

        function resetInactivityTimer() {
            if (isLocked) return;
            clearTimeout(inactivityTimer);
            inactivityTimer = setTimeout(triggerLock, inactiveLimit);
        }

        ["mousemove", "keydown", "click", "scroll"].forEach(evt =>
            document.addEventListener(evt, resetInactivityTimer)
        );

        window.onload = resetInactivityTimer;

    </script>

