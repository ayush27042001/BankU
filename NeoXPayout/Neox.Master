    <style>
        .bank-transfer-row {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 12px 16px;
            cursor: pointer;
            transition: background 0.2s ease, transform 0.2s ease;
        }

            .bank-transfer-row:hover {
                background: #f0f8ff;
                transform: translateX(3px);
                border-radius: 8px;
            }

        .modal-backdrop {
            backdrop-filter: blur(100px);
            -webkit-backdrop-filter: blur(100px);
            background-color: rgba(0, 0, 0, 0.55) !important;
        }

        #mpinModal.modal {
            z-index: 9999 !important; /* very high to stay on top */
        }

        /* Make sure the backdrop is also above other content */
        #mpinModal + .modal-backdrop {
            z-index: 9998 !important;
        }

        .wallet-balance {
            display: inline-flex;
            align-items: center;
            gap: 8px;
            font-size: 24px;
            font-weight: 600;
            color: #1e1e1e;
        }

        .wallet-amount {
            font-weight: 600;
            font-size: 24px;
            color: #1e1e1e;
        }

        .reload-btn {
            background: none;
            border: none;
            color: #4a4a4a;
            font-size: 20px;
            cursor: pointer;
            transition: all 0.3s ease;
            padding: 4px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
        }

            .reload-btn:hover {
                background-color: #f2f2f2;
                color: #6a008a; /* purple hover */
                transform: rotate(180deg);
            }

        .reload-icon {
            display: inline-block;
            transition: transform 0.4s ease;
        }

        .reload-btn:active .reload-icon {
            transform: rotate(360deg);
        }
    </style>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>

</head>
